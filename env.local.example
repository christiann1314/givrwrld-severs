# GIVRwrld Local Development Environment
# Copy this to .env.local and update with your actual values

# API Configuration (MySQL-backed API server)
VITE_API_URL=http://localhost:3001

# Stripe Configuration
# IMPORTANT: Set your Stripe publishable key (pk_live_... for production, pk_test_... for development)
VITE_STRIPE_PUBLISHABLE_KEY=

# Panel Configuration
VITE_PANEL_URL=https://panel.givrwrldservers.com
VITE_PTERO_CLIENT_KEY=your_pterodactyl_client_key_here

# App Configuration
VITE_APP_NAME=GIVRwrld Servers
VITE_APP_URL=http://localhost:8080
